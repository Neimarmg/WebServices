<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from calculadoraService WSDL service -->

  <service name="calculadora" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://service.calc.com"/>
      <parameter name="wsdlServiceElement" value="calculadoraService"/>
      <parameter name="schemaQualified" value="http://service.calc.com"/>
      <parameter name="wsdlServicePort" value="calculadora"/>
      <parameter name="className" value="com.calc.service.CalculadoraSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="calculadora"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getResultado" qname="operNS:getResultado" xmlns:operNS="http://service.calc.com" returnQName="retNS:getResultadoReturn" xmlns:retNS="http://service.calc.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:valor1" xmlns:pns="http://service.calc.com" type="tns:float" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:valor2" xmlns:pns="http://service.calc.com" type="tns:float" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getResultado"/>

  </service>
</deployment>
