<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from servicoCalculoService WSDL service -->

  <service name="servicoCalculo" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://calculos.com"/>
      <parameter name="wsdlServiceElement" value="servicoCalculoService"/>
      <parameter name="schemaQualified" value="http://calculos.com"/>
      <parameter name="wsdlServicePort" value="servicoCalculo"/>
      <parameter name="className" value="com.calculos.ServicoCalculoSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="servicoCalculo"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getResultado" qname="operNS:getResultado" xmlns:operNS="http://calculos.com" returnQName="retNS:getResultadoReturn" xmlns:retNS="http://calculos.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:operacao" xmlns:pns="http://calculos.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:valor1" xmlns:pns="http://calculos.com" type="tns:float" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:valor2" xmlns:pns="http://calculos.com" type="tns:float" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getResultado"/>

  </service>
</deployment>
