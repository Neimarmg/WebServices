<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from calcwsService WSDL service -->

  <service name="calcws" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://ws.sercice.com"/>
      <parameter name="wsdlServiceElement" value="calcwsService"/>
      <parameter name="schemaQualified" value="http://ws.sercice.com"/>
      <parameter name="wsdlServicePort" value="calcws"/>
      <parameter name="className" value="com.sercice.ws.CalcwsSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="calcws"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="wsRetorno" qname="operNS:wsRetorno" xmlns:operNS="http://ws.sercice.com" returnQName="retNS:wsRetornoReturn" xmlns:retNS="http://ws.sercice.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:desc" xmlns:pns="http://ws.sercice.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="wsRetorno"/>

  </service>
</deployment>
